{% extends 'base.html.twig' %}

{% block title %}Taxe
{% endblock %}

{% block body %}
	<div class="container-fluid dashboard-div">
		<div class="row top-actions">
			<div class="col-8 top-header">
				<h4>Taxe</h4>
			</div>

		</div>

		<table class="table">
			<tbody>
				<tr>
					<th>Nom</th>
					<td>{{ taxe.nom }}</td>
				</tr>
				<tr>
					<th>Taux</th>
					<td>{{ taxe.taux }}</td>
				</tr>
			</tbody>
		</table>

         <a class="btn btn-danger" href="{{ path('taxe_index') }}"><i class="fas fa-chevron-circle-left"></i></a>
        <div style="float: right;" class="col-2 actions text-right">
            <a class="btn btn-warning" href="{{ path('taxe_edit', {'id': taxe.id}) }}"><i
                        class="fas fa-edit"></i></a>
        </div>

	</div>
{% endblock %}
