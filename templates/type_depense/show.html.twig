{% extends 'base.html.twig' %}

{% block title %}Type Depense
{% endblock %}

{% block body %}
	<div class="container-fluid dashboard-div">
		<div class="row top-actions">
			<div class="col-8 top-header">
				<h4>Type Depense</h4>
			</div>

		</div>
		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ type_depense.id }}</td>
				</tr>
				<tr>
					<th>Nom</th>
					<td>{{ type_depense.nom }}</td>
				</tr>
				<tr>
					<th>Description</th>
					<td>{{ type_depense.description }}</td>
				</tr>
			</tbody>
		</table>

        <a class="btn btn-danger" href="{{ path('type_depense_index') }}"><i class="fas fa-chevron-circle-left"></i></a>
        <div style="float: right;" class="col-2 actions text-right">
            <a class="btn btn-warning" href="{{ path('type_depense_edit', {'id': type_depense.id}) }}"><i
                        class="fas fa-edit"></i></a>
        </div>
	</div>
{% endblock %}
